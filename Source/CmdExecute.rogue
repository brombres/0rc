module Orc

augment
  METHODS
    method Cmd.execute->OrcObject
      throw t.error( "[INTERNAL] $.execute() is undefined."(type_name) )

    method LiteralInt.execute->OrcObject
      return OrcInt( value )

    method Negate.execute->OrcObject
      return -operand.execute

    method Statements.execute->OrcObject
      local result = OrcUndefined : OrcObject
      forEach (statement in this)
        result = statement.execute
      endForEach
      return result

endAugment
